{
    "redirects": [
      {
        "source": "/index_en.html",
        "destination": "/?lan=en",
        "permanent": true
      }
    ],
    "rewrites": [
      {
        "source": "/",
        "has": [
          {
            "type": "query",
            "key": "lan",
            "value": "en"
          }
        ],
        "destination": "/index_en.html"
      },
      {
        "source": "/",
        "has": [
          {
            "type": "query",
            "key": "lan",
            "value": "ua"
          }
        ],
        "destination": "/index.html"
      },
      {
        "source": "/",
        "destination": "/index.html"
      }
    ]
  }
  