{
    "rewrites": [
      {
        "source": "/",
        "destination": "/index.html",
        "has": [
          {
            "type": "query",
            "key": "lan",
            "value": "(ua|pl|it|ger)"
          }
        ]
      },
      {
        "source": "/",
        "destination": "/index_en.html",
        "has": [
          {
            "type": "query",
            "key": "lan",
            "value": "en"
          }
        ]
      },
      {
        "source": "/",
        "destination": "/index.html"
      }
    ]
  }
  